    "mongoose-gridfs": "^1.3.0",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^1.3.0",
    "nodemailer": "^6.9.13",
    "react-bootstrap": "^2.10.2",
    "react-datepicker": "^6.9.0",
    "react-rating": "^2.0.5",
    "react-speech-recognition": "^3.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.0",
    "react-scripts": "^3.0.1"
  }
